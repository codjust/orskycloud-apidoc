[
  {
    "type": "POST",
    "url": "http://hcwzq.cn/api/getDeviceData.json?uid=*&did=*[&StartTime=*][&EndTime=*]",
    "title": "getDeviceData",
    "name": "getDeviceData",
    "group": "All",
    "version": "1.0.1",
    "description": "<p>获取指定设备的全部传感器数据</p>",
    "parameter": {
      "fields": {
